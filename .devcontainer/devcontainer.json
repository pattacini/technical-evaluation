{
  "name": "Technical Evaluation",
  "image": "ghcr.io/pattacini/technical-evaluation:codespaces",
  "forwardPorts": [8080, 8888, 5901, 6080, 10000],
  "remoteUser": "codespace",
  "customizations": {
    "codespaces": {
      "openFiles": ["README.md"]
    }
  },
  "postAttachCommand": {
    "markserver": "start-markserv.sh",
    "jupyter": "start-jupyter.sh",
    "vnc": "start-vnc-session.sh",
    "console": "jq -r \".CODESPACE_NAME\" /workspaces/.codespaces/shared/environment-variables.json"
  },
  "workspaceFolder": "/workspaces"
}
